datasource db {
  provider = "postgresql"
  url      = "postgres://root:prisma@0.0.0.0:5433/prisma?schema=postgres-table-1-to-1%24dev"
}

model Settings {
  id   String @default(cuid()) @id
  User User[] @relation("Settings", references: [id])
}

model User {
  id       String     @default(cuid()) @id
  Settings Settings[] @relation("Settings", references: [id])
}
